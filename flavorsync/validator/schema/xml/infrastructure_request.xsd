<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified">
    <xs:complexType name="infrastructureType">
        <xs:sequence>
            <xs:element type="xs:string" name="name" minOccurs="1" maxOccurs="1"/>
            <xs:element type="xs:anyURI" name="keystone_url" minOccurs="1" maxOccurs="1"/>
            <xs:element type="xs:string" name="username" minOccurs="1" maxOccurs="1"/>
            <xs:element type="xs:string" name="password" minOccurs="1" maxOccurs="1"/>
            <xs:element type="xs:string" name="tenant" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element type="infrastructureType" name="infrastructure"/>
</xs:schema>
