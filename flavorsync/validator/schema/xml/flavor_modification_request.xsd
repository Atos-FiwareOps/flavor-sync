        <xs:sequence>
            <xs:element type="xs:boolean" name="promoted" minOccurs="0" maxOccurs="1"/>
            <xs:element type="xs:boolean" name="public" minOccurs="0" maxOccurs="1"/>
            <xs:element type="xs:string" name="node" minOccurs="0" maxOccurs="1"/>
